{
    "name": "vue3-cli5-template",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "serve:stage": "vue-cli-service serve --mode staging",
        "serve:prod": "vue-cli-service serve --mode production",
        "build": "vue-cli-service build",
        "build:stage": "vue-cli-service build --mode staging",
        "build:prod": "vue-cli-service build --mode production",
        "deploy": "gupo-deploy --mode staging -p",
        "deploy:prod": "gupo-deploy --mode production -p",
        "commit": "git-cz",
        "lint": "vue-cli-service lint",
        "lint:ls-lint": "ls-lint",
        "lint:prettier": "prettier --write . --loglevel warn --ignore-unknown",
        "lint:stylelint": "stylelint --fix \"**/*.{css,scss,sass,less,styl,html,vue}\" --cache --cache-location node_modules/.cache/stylelint/",
        "pre-commit:lint-staged": "lint-staged",
        "prepare": "husky install"
    },
    "dependencies": {
        "@vueuse/components": "8.2.4",
        "@vueuse/core": "8.2.3",
        "axios": "^0.24.0",
        "core-js": "^3.8.3",
        "pinia": "2.0.12",
        "pinia-plugin-persistedstate": "1.5.1",
        "secure-ls": "^1.2.6",
        "vue": "^3.2.13",
        "vue-router": "^4.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@babel/eslint-parser": "^7.12.16",
        "@commitlint/cli": "^16.0.2",
        "@commitlint/config-conventional": "^16.0.0",
        "@ls-lint/ls-lint": "^1.10.0",
        "@vue/cli-plugin-babel": "~5.0.1",
        "@vue/cli-plugin-eslint": "~5.0.1",
        "@vue/cli-plugin-router": "~5.0.1",
        "@vue/cli-service": "~5.0.1",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "babel-plugin-transform-remove-debugger": "^6.9.4",
        "commitizen": "^4.2.4",
        "compression-webpack-plugin": "^9.2.0",
        "cz-conventional-changelog": "3.3.0",
        "cz-customizable": "^6.3.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^8.0.3",
        "gupo-deploy": "^1.0.8",
        "husky": "^7.0.0",
        "less": "^4.0.0",
        "less-loader": "^8.0.0",
        "lint-staged": "12.4.0",
        "postcss-html": "^1.3.0",
        "postcss-less": "^6.0.0",
        "prettier": "^2.4.1",
        "style-resources-loader": "^1.4.1",
        "stylelint": "^14.2.0",
        "stylelint-config-html": "^1.0.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recommended-vue": "^1.1.0",
        "stylelint-config-standard": "^23.0.0",
        "stylelint-order": "^5.0.0",
        "svg-sprite-loader": "^6.0.11",
        "svgo-loader": "^3.0.0",
        "vue-cli-plugin-style-resources-loader": "~0.1.5",
        "webpack-bundle-analyzer": "^4.5.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-customizable"
        }
    }
}
