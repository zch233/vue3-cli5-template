{
    "name": "vue3-cli5-template",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "lint:prettier": "prettier --write --loglevel warn \"src/**/*\"",
        "lint:ls-lint": "ls-lint",
        "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
        "pre-commit:pretty": "pretty-quick --staged",
        "pre-commit:lint-staged": "lint-staged",
        "commit": "git-cz",
        "prepare": "husky install",
        "deploy": "gupo-deploy --mode staging -p",
        "deploy:prod": "gupo-deploy --mode production -p"
    },
    "dependencies": {
        "core-js": "^3.8.3",
        "vue": "^3.2.13",
        "vue-router": "^4.0.3",
        "vuex": "^4.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@babel/eslint-parser": "^7.12.16",
        "@commitlint/cli": "^16.0.2",
        "@commitlint/config-conventional": "^16.0.0",
        "@ls-lint/ls-lint": "^1.10.0",
        "@vue/cli-plugin-babel": "~5.0.0-rc.1",
        "@vue/cli-plugin-eslint": "~5.0.0-rc.1",
        "@vue/cli-plugin-router": "~5.0.0-rc.1",
        "@vue/cli-plugin-vuex": "~5.0.0-rc.1",
        "@vue/cli-service": "~5.0.0-rc.1",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "babel-plugin-transform-remove-debugger": "^6.9.4",
        "commitizen": "^4.2.4",
        "compression-webpack-plugin": "^9.2.0",
        "cz-conventional-changelog": "3.3.0",
        "cz-customizable": "^6.3.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^8.0.3",
        "gupo-deploy": "^1.0.8",
        "husky": "^7.0.0",
        "less": "^4.0.0",
        "less-loader": "^8.0.0",
        "lint-staged": "^11.1.2",
        "postcss-html": "^1.3.0",
        "postcss-less": "^6.0.0",
        "prettier": "^2.4.1",
        "pretty-quick": "^3.1.3",
        "stylelint": "^14.2.0",
        "stylelint-config-html": "^1.0.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recommended-vue": "^1.1.0",
        "stylelint-config-standard": "^23.0.0",
        "stylelint-order": "^5.0.0",
        "webpack-bundle-analyzer": "^4.5.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-customizable"
        }
    }
}
